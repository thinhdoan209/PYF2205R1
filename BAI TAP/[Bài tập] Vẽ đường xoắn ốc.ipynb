   "source": [
    "d0 = float(input(\"Nhập vào khoảng cách tối đa: \"))\n",
    "\n",
    "import turtle\n",
    "t = turtle.Turtle()\n",
    "a = 2\n",
    "\n",
    "while True:\n",
    "    t.forward(a)\n",
    "    a += 0.08\n",
    "    t.left(10)\n",
    "    if(turtle.distance(t)>d0):\n",
    "        break\n",
    "        \n",
    "turtle.done()"
   ]
